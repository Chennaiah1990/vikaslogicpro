package patternPrograms;

public class fourty {
	public static void main(String args[])
	{
		char n='E',z='A';
		for(char i='A';i<n;i++)
		{
			for(char j=(char) (n-'E');j>i;j--)
			{
				System.out.print(" ");
			}
			for(char k='A';k<z;k++)
			{
				System.out.print(i);
			}
			z+=2;
			System.out.println();
		}
	}
}
