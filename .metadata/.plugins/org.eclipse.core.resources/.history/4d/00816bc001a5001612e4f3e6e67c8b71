package patternPrograms;

public class theartyfive {
	public static void main(String args[])
	{
		for(char i='A';i<='E';i++)
		{
			for(char j='E';j>i;j--)
			{
				System.out.print(" ");
			}
			for(char k='A';k<=i;k++)
			{
				if(i%2!=0)
				System.out.print(i);
			}
			for(char z='B';z<=i;z++)
			{
				if(i%2!=0)
				System.out.print(i);
			}
			System.out.println();
		}
	}

}
