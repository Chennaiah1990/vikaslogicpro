package patternPrograms;

public class thaertysix {
	public static void main(String args[])
	{
		for(int i=0;i<=5;i++)
		{
			for(int j=5;j>i;j--)
			{
				System.out.print(" ");
			}
			for(int k=0;k<=i;k++)
			{
				if(i%2!=0)
				System.out.print(i);
			}
			for(int z=1;z<=i;z++)
			{
				if(i%2!=0)
				System.out.print(i);
			}
			System.out.println();
		}
	}

}
